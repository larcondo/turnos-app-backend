{
  "compilerOptions": {
    /* Language and Environment */ 
    "target": "ES2022",
    /* Modules */
    "module": "CommonJS",
    "moduleResolution": "node",
    /* Emit */
    "outDir": "./build/",
    /* Interop Constraints */
    "esModuleInterop": true,
    /* Type Checking */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true
  }
}